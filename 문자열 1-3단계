// 11654
#include <stdio.h>
#include <string.h>

main() {
	int s;
	scanf_s("%c", &s);
	printf("%d", s);

}


//11720
#include<stdio.h>

main() {

    int n;
    int total = 0;
    char a[100];

    scanf_s("%d", &n);
    scanf_s("%s", a);
   

    for (int i = 0; i < n; i++) {
  
        total += (a[i]-'0');
    }

    printf("%d", total);
    return 0;
}


//10809
#include <stdio.h>

main()
{
	char p[101] = "";
	int a[26] = { 0, };
	int i;
	int j;

	scanf_s("%s", p);

	for (i = 'a'; i <= 'z'; i++) {
		for (j = 0; j < 100; j++) {
			if (p[j] == i) {
				a[i - 97] = j + 1;
				break;
			}
		}
	}

	for (i = 0; i < 26; i++) {
		printf("%d ", a[i] - 1);
	}
	printf("\n");

	return 0;
}
