//2438
#include<stdio.h>

main(){
    int i;
    int a;
    int j;
    
    scanf("%d", &a);
    
    for(i=1; i<=a; i++){
        for(j=1; j<=i; j++){
        printf("*");
        }
        printf("\n");
    }
}

//2439
#include <stdio.h>
 
main() {
    int i, j;
    int a, n;
    
    scanf("%d", &n);
    
    for(i=1; i<= n; i++ ) {
    	for(a=0; a<n-i; a++) {
    		printf(" ");
    	}
    	for(j=0; j<i; j++ ) {
    		printf("*");
    	}
    	printf("\n");
    }
}

//10871
#include<stdio.h>

int main(){
    int a, b, n;
    int i;

    scanf("%d %d", &a, &b);

    for(i=0; i<a; i++){
        scanf("%d", &n);
      
    if(b>n)
    printf("%d ", n);
    }
}
