//2739

#include <stdio.h>

main(){
    int a, i;
    
    scanf_s("%d",&a);
    for(i=1; i<=9; i++){
        printf("%d * %d = %d\n",a,i,a*i);
    }
}


//10950

#include<stdio.h>

main(){
    int a,b,n,i;
    scanf_s("%d",&n);

    for(i=0; i<n; i++){
        scanf("%d %d",&a,&b);
        printf("%d\n",a+b);
    }
}


//8393

#include <stdio.h>

main(){
    int n,i;
    int s = 0;
    scanf("%d", &n);
    
    for(i=1; i <= n; i++){
        s+=i;
    }
    printf("%d",s);
}


